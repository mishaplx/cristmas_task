{
    "plugins": ["prettier", "import"],
    "extends": ["plugin:prettier/recommended", "prettier", "airbnb-typescript"],
    "parserOptions": {
      "project": "tsconfig.json",
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": 0,
      "no-explicit-any": "on",
      "class-methods-use-this": "off",
      "react/jsx-filename-extension": [0],
      "import/extensions": "off"
    }
  }